{
  "name": "adrsbook-front-end-serverside-rendering",
  "version": "0.1.0",
  "description": "adrsbook app's front-end using serverside rendering",
  "main": "index.js",
  "scripts": {
    "build:ssr": "webpack --config webpack.config.ssr.js",
    "build:app-dev": "webpack -p --config webpack.config.dev.js",
    "build:dev": "run-s build:ssr build:app-dev",
    "build:prod": "webpack --config webpack.config.prod.js",
    "prebuild": "npm run build:ssr",
    "build": "npm run build:prod",
    "generate-random-data": "node ./mockAPI/generate.js",
    "prestart:api": "npm run generate-random-data",
    "start:api": "json-server --watch ./mockAPI/db.json --port 2004 --routes ./mockAPI/routes.json",
    "start:dev-server": "webpack-dev-server --config webpack.config.dev.js",
    "start": "run-p start:api start:dev-server",
    "test": "jest --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvnam96/addressbook--server-side.git"
  },
