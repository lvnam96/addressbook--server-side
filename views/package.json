{
  "name": "adrsbook-front-end-serverside-rendering",
  "version": "0.1.0",
  "description": "adrsbook app's front-end using serverside rendering",
  "main": "index.js",
  "scripts": {
    "webpack-ssr": "webpack --config webpack.config.ssr.js",
    "build-ssr": "NODE_ENV=production mkdir -p ssr && npm run webpack-ssr",
    "build-prod": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build-dev": "webpack -p --config webpack.config.dev.js",
    "prebuild": "npm run build-ssr",
    "build": "npm run build-prod",
    "start": "npm run mock-api | webpack-dev-server --config webpack.config.dev.js",
    "generate-random-data": "node ./mockAPI/generate.js",
    "premock-api": "npm run generate-random-data",
    "mock-api": "json-server --watch ./mockAPI/db.json --port 2004 --routes ./mockAPI/routes.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvnam96/addressbook--server-side.git"
